DROP TABLE IF EXISTS `cts_exec`.`result`;
CREATE TABLE  `cts_exec`.`result` (
  `id` int(11) NOT NULL,
  `idtest` int(11) NOT NULL,
  `resulttest` varchar(200) NOT NULL,
  `comment` text NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
